{
    "basePath": "http://10.0.1.150:8000/v1/",
    "apiVersion": "1",
    "resourcePath": "/auth",
    "apis": [
        {
            "path": "/auth/",
            "operations": [
                {
                    "httpMethod": "POST",
                    "summary": "Register.",
                    "nickname": "Register",
                    "responseClass": "void",
                    "parameters": [
                        {
                            "name": "json",
                            "description": "json",
                            "allowMultiple": false,
                            "dataType": "registerParams",
                            "paramType": "body",
                            "required": true
                        }
                    ],
                    "errorResponses": [
                        {
                            "reason": "Not all params correct.",
                            "code": 400
                        },
                        {
                            "reason": "User exist.",
                            "code": 300
                        }
                    ],
                    "notes": ""
                }
            ]
        },
        {
            "path": "/auth/login/",
            "operations": [
                {
                    "httpMethod": "PUT",
                    "summary": "Login.",
                    "nickname": "login",
                    "responseClass": "void",
                    "parameters": [
                        {
                            "name": "json",
                            "description": "json",
                            "allowMultiple": false,
                            "dataType": "loginParams",
                            "paramType": "body",
                            "required": true
                        }
                    ],
                    "errorResponses": [
                        {
                            "reason": "Invalid login type.",
                            "code": 400
                        },
                        {
                            "reason": "Token not correct.",
                            "code": 409
                        },
                        {
                            "reason": "User not registered.",
                            "code": 404
                        },
                        {
                            "reason": "User not found on Facebook.",
                            "code": 405
                        }
                    ],
                    "notes": ""
                }
            ]
        }
    ],
    "models": {
        "registerParams": {
            "id": "registerParams",
            "properties": {
                "name": {
                    "type": "string"
                },
                "lastname": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "industry_id": {
                    "type": "int"
                },
                "summary": {
                    "type": "string"
                },
                "photo_id": {
                    "type": "int"
                },
                "phone": {
                    "type": "string"
                },
                "business_email": {
                    "type": "string"
                },
                "skype": {
                    "type": "string"
                },
                "city": {
                    "type": "string"
                },
                "facebook_key": {
                    "type": "string"
                },
                "facebook_id": {
                    "type": "string"
                },
                "linkedin_id": {
                    "type": "string"
                },
                "linkedin_key": {
                    "type": "string"
                },
                "skills": {
                    "type": "Array()"
                },
                "languages": {
                    "type": "Array()"
                },
                "jobs": {
                    "type": "Array",
                    "items": {
                        "$ref": "jobsParams"
                    }
                },
                "education": {
                    "type": "Array",
                    "items": {
                        "$ref": "educationParams"
                    }
                }
            }
        },
        "jobsParams": {
            "id": "jobsParams",
            "properties": {
                "name": {
                    "type": "string"
                },
                "company": {
                    "type": "string"
                },
                "current": {
                    "type": "string"
                },
                "start_time": {
                    "type": "string"
                },
                "end_time": {
                    "type": "string"
                }
            }
        },
        "educationParams": {
            "id": "educationParams",
            "properties": {
                "name": {
                    "type": "string"
                },
                "company": {
                    "type": "string"
                },
                "start_time": {
                    "type": "string"
                },
                "end_time": {
                    "type": "string"
                }
            }
        },
        "loginParams": {
            "id": "loginParams",
            "properties": {
                "type": {
                    "type": "int"
                },
                "token": {
                    "type": "string"
                }
            }
        }
    }
}