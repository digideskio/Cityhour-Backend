{
    "basePath": "http://test.mockup.io/v1/",
    "apiVersion": "1",
    "resourcePath": "/uploads",
    "apis": [
        {
            "path": "/uploads/",
            "operations": [
                {
                    "httpMethod": "POST",
                    "summary": "Upload images.",
                    "nickname": "UploadImages",
                    "responseClass": "UpModel",
                    "parameters": [
                        {
                            "name": "project",
                            "description": "Project ID",
                            "allowMultiple": false,
                            "dataType": "int",
                            "paramType": "query",
                            "required": true
                        },
                        {
                            "name": "img",
                            "description": "image like multipart form data",
                            "allowMultiple": false,
                            "dataType": "string",
                            "paramType": "query",
                            "required": false
                        },
                        {
                            "name": "tags",
                            "description": "tags id array. Example:  1,2,3,4",
                            "allowMultiple": false,
                            "dataType": "string",
                            "paramType": "query",
                            "required": true
                        },
                        {
                            "name": "fakeMockId",
                            "description": "Temporary mock ID",
                            "allowMultiple": false,
                            "dataType": "string",
                            "paramType": "query",
                            "required": true
                        },
                        {
                            "name": "folder_id",
                            "description": "Folder ID",
                            "allowMultiple": false,
                            "dataType": "int",
                            "paramType": "query",
                            "required": true
                        }
                    ],
                    "errorResponses": [
                        {
                            "reason": "Have no permissions",
                            "code": 401
                        }
                    ]
                }
            ]
        },
        {
            "path": "/uploads/{id}",
            "operations": [
                {
                    "httpMethod": "PUT",
                    "summary": "Update image.",
                    "nickname": "UpdateImage",
                    "responseClass": "UpModel",
                    "parameters": [
                        {
                            "name": "project",
                            "description": "Project ID",
                            "allowMultiple": false,
                            "dataType": "int",
                            "paramType": "query",
                            "required": true
                        },
                        {
                            "name": "id",
                            "description": "Mockup ID",
                            "allowMultiple": false,
                            "dataType": "int",
                            "paramType": "path",
                            "required": true
                        },
                        {
                            "name": "single",
                            "description": "for this type of request must be true.",
                            "allowMultiple": false,
                            "dataType": "string",
                            "paramType": "query",
                            "required": true
                        },
                        {
                            "name": "img",
                            "description": "image like multipart form data",
                            "allowMultiple": false,
                            "dataType": "string",
                            "paramType": "query",
                            "required": false
                        },
                        {
                            "name": "folder_id",
                            "description": "Folder ID",
                            "allowMultiple": false,
                            "dataType": "int",
                            "paramType": "query",
                            "required": true
                        }
                    ],
                    "errorResponses": [
                        {
                            "reason": "Have no permissions",
                            "code": 401
                        }
                    ]
                }
            ]
        }
    ],
    "models": {
        "UpModel": {
            "id": "UpModel",
            "properties": {
                "project": {
                    "type": "int"
                },
                "images": {
                    "type": "dictionary(full:string,half:string,thumb:string)"
                },
                "name": {
                    "type": "string"
                },
                "file_size": {
                    "type": "int"
                },
                "device": {
                    "type": "int"
                },
                "folder_id": {
                    "type": "int"
                },
                "status_bar": {
                    "type": "int"
                },
                "exact_size": {
                    "type": "int"
                },
                "order": {
                    "type": "int"
                },
                "width": {
                    "type": "int"
                },
                "zone_bottom": {
                    "type": "int"
                },
                "zone_top": {
                    "type": "int"
                },
                "height": {
                    "type": "int"
                },
                "id": {
                    "type": "int"
                }
            }
        }
    }
}