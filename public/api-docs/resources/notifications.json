{
    "basePath": "http://test.mockup.io/v1/",
    "apiVersion": "1",
    "resourcePath": "/notifications",
    "apis": [
        {
            "path": "/notifications/",
            "operations": [
                {
                    "httpMethod": "GET",
                    "summary": "Unsubscribe.",
                    "nickname": "Unsubscribe",
                    "responseClass": "void",
                    "parameters": [
                        {
                            "name": "type",
                            "description": "Type of subscription",
                            "allowMultiple": false,
                            "dataType": "int",
                            "paramType": "query",
                            "required": true
                        },
                        {
                            "name": "key",
                            "description": "User personal api key",
                            "allowMultiple": false,
                            "dataType": "string",
                            "paramType": "query",
                            "required": true
                        }
                    ]
                },
                {
                    "httpMethod": "POST",
                    "summary": "Notify registration open.",
                    "nickname": "RegistrationOpen",
                    "responseClass": "void",
                    "parameters": [
                        {
                            "name": "email",
                            "description": "Email",
                            "allowMultiple": false,
                            "dataType": "string",
                            "paramType": "query",
                            "required": true
                        }
                    ],
                    "errorResponses": [
                        {
                            "reason": "Server error.",
                            "code": 500
                        },
                        {
                            "reason": "Bad email.",
                            "code": 409
                        },
                        {
                            "reason": "User exist.",
                            "code": 417
                        }
                    ]
                }
            ]
        }
    ]
}