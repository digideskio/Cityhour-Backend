{
    "basePath": "http://test.mockup.io/v1/",
    "apiVersion": "1",
    "resourcePath": "/icons",
    "apis": [
        {
            "path": "/icons/",
            "operations": [
                {
                    "httpMethod": "POST",
                    "summary": "Upload icons.",
                    "nickname": "UploadIcons",
                    "responseClass": "void",
                    "parameters": [
                        {
                            "name": "project",
                            "description": "id of project",
                            "allowMultiple": false,
                            "dataType": "int",
                            "paramType": "query",
                            "required": true
                        },
                        {
                            "name": "img",
                            "description": "image like multipart form data",
                            "allowMultiple": false,
                            "dataType": "string",
                            "paramType": "body",
                            "required": false
                        }
                    ],
                    "errorResponses": [
                        {
                            "reason": "Have no permissions",
                            "code": 401
                        }
                    ]
                }
            ]
        },
        {
            "path": "/icons/{id}",
            "operations": [
                {
                    "httpMethod": "DELETE",
                    "summary": "Delete Icon.",
                    "nickname": "DeleteIcon",
                    "responseClass": "void",
                    "parameters": [
                        {
                            "name": "project",
                            "description": "id of project",
                            "allowMultiple": false,
                            "dataType": "int",
                            "paramType": "path",
                            "required": true
                        }
                    ],
                    "errorResponses": [
                        {
                            "reason": "Have no permissions",
                            "code": 401
                        }
                    ]
                }
            ]
        }
    ]
}